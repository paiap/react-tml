{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["es6","DOM"],
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "build",
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules","build/**/*"]
}